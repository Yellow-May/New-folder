{
  "name": "asceta-backend",
  "version": "1.0.0",
  "description": "ASCETA Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "yarn migration:up:prod && yarn seed:prod && node dist/server.js",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:up:prod": "migrate-mongo up",
    "migration:down": "migrate-mongo down",
    "migration:status": "migrate-mongo status",
    "migration": "yarn migration:up",
    "migrate": "yarn migration:up",
    "seed": "ts-node src/seed.ts",
    "seed:prod": "node dist/seed.js",
    "setup": "yarn migration:up && yarn seed",
    "setup:prod": "yarn migration:up:prod && yarn seed:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "migrate-mongo": "^11.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/migrate-mongo": "^8.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
